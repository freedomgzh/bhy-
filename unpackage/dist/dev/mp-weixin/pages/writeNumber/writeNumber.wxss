
page {
	height: 100%;
}
.content {
	width: 100%;
	height: 100%;
	background-color: #F6F6F6;
	padding-top: 20rpx;
}
.listBox {

	background-color: #FFFFFF;
	width: 100%;
	height: calc(100% - 20rpx);
	margin-bottom: 200rpx;
}
.list {
	height: 147rpx;
	margin: 0rpx 20rpx 0rpx 20rpx;
	border-bottom: 1rpx solid#EEEEEE;
}
.camera {
	width: 128rpx;
	height: 88rpx;
	border: 1rpx solid#DDDDDD;
	border-radius: 10rpx;
}
.camera view {
	font-size: 22rpx;
	color: #CACACA;
}
.cameraImg {
	width: 45rpx;
	height: 39rpx;
}
.countBtns image {
	width: 46rpx;
	height: 46rpx;
}
.inputBox {
	height: 88rpx;
	margin: 0 31rpx 0 19rpx;
	border: 1rpx solid##DDDDDD;
	border-radius: 10rpx;
	font-size: 28rpx;
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
}
.inputBox input {
	color: #CACACA;
}
.countImg {
	margin-right: 30rpx;
}
.btns {
	height: 100%;
}
.btnsubmit{
	position: absolute;
	bottom: 60rpx;
	left: 20rpx;
	height: 84rpx;
	width: 710rpx;
	border-radius: 10rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 84rpx;
	background-color: #005EA1;
	font-size: 32rpx;
}

