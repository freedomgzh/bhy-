<view class="content"><view class="top"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{imglist}}" wx:for-item="s" wx:for-index="i" wx:key="i"><swiper-item><image class="aimg" src="{{s.img}}" mode></image></swiper-item></block></swiper></view><view class="search flexYc"><image class="spic" src="../../static/搜索%20(7)@3x.png" mode></image><input class="inputs" type="text" disabled="true" placeholder-style="color: #DDDDDD;" value="" placeholder="搜索厂家" data-event-opts="{{[['tap',[['toSearch',['$event']]]]]}}" bindtap="__e"/></view><view class="title flexXb"><view class="o">合作厂家</view><view data-event-opts="{{[['tap',[['toList',['$event']]]]]}}" class="n" bindtap="__e"><text>查看更多</text><text>></text></view></view><view class="listBox"><block wx:for="{{list}}" wx:for-item="s" wx:for-index="i" wx:key="i"><view class="list flexYc"><image class="logoImg" src="{{s.img}}" mode></image><view class="flexCol"><text class="name">{{s.name}}</text><text class="address">{{"地址："+s.address}}</text></view></view></block></view><image class="btns" src="../../static/toOrder.png" mode data-event-opts="{{[['tap',[['toOrder',['$event']]]]]}}" bindtap="__e"></image></view>