<view class="content"><block wx:for="{{list}}" wx:for-item="s" wx:for-index="i" wx:key="i"><view class="list"><view class="listContent"><view class="top flexYc flexXb"><view class="flex flexYc"><view>{{''+s.factoryName+''}}</view></view><view class="status">{{''+s.status+''}}</view></view><view class="label flexXb"><text>{{"到厂时间："+s.daochangTime}}</text><text>{{"监督项："+s.workContent}}</text></view><view class="label flexXb"><text>{{"驾驶员："+s.driverName}}</text><text>{{"随车电话："+s.mobile}}</text></view><view class="label flexXb"><text>{{"运输物："+s.goodsName}}</text><text>{{"车牌号："+s.carNumber}}</text></view></view><view class="btnsBox flexXb flexYc"><view data-event-opts="{{[['tap',[['getNumber',['$0'],[[['list','',i,'id']]]]]]]}}" class="btns flexYc flexXc" bindtap="__e">查看铅封号</view><view data-event-opts="{{[['tap',[['wQvideo',['$0'],[[['list','',i,'id']]]]]]]}}" class="btns flexYc flexXc" bindtap="__e">查看铅封视频</view><view data-event-opts="{{[['tap',[['wCvideo',['$0'],[[['list','',i,'id']]]]]]]}}" class="btns flexYc flexXc" bindtap="__e">查看清罐视频</view></view></view></block><block wx:if="{{showVideo}}"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="videoBox" style="{{'top:'+(height+'px')+';'}}" bindtap="__e"><view class="chooseContent"><view class="title flexXa"><view></view><view>查看铅封视频</view><image class="closeImg" src="../../../static/close.png" mode></image></view><block wx:for="{{vlist}}" wx:for-item="s" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['choose',[i]]]]]}}" class="box" bindtap="__e">{{"视频"+(i+1)}}</view></block></view></view></block><block wx:if="{{hasChoose}}"><view class="videoBoxs" style="{{'top:'+(height+'px')+';'}}"><view data-event-opts="{{[['tap',[['closeBox',['$event']]]]]}}" class="close flexXc flexYc" bindtap="__e"><image class="closeBtn" src="../../../static/close.png" mode></image></view><block wx:if="{{hasChoose}}"><video id="myVideo" src="{{vlist[index].url}}" danmu-list="{{danmuList}}" enable-danmu="{{true}}" danmu-btn="{{true}}" controls="{{true}}" data-event-opts="{{[['error',[['videoErrorCallback',['$event']]]]]}}" binderror="__e"></video></block></view></block></view>