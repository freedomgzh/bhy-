	color: #999999;
}
.list{
	margin-top: 20rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
	background-color: #FFFFFF;
	border-radius: 20rpx;
}
.lab{
	margin-top: 32rpx;
}
.active{
	color: #000000;
}
.activeBottom{
	width: 34rpx;
