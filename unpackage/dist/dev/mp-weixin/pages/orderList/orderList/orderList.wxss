}
.top{
	font-size: 28rpx;
	height: 100rpx;
	/* border-bottom: 1upx solid#EEEEEE; */
}
.logo {
	height: 80rpx;
	width: 80rpx;
	margin:0 21rpx;
}
.status{
	color: #005EA1;
	font-size: 26rpx;
	margin-right: 25rpx;
}
.listContent{
	font-size: 26rpx;
	padding-top:10rpx;
	padding-left: 18rpx;
}
.label{
	line-height: 60rpx;
}
.label text{
	width: 50%;
}
.btnsBox{
	height: 100rpx;
	padding: 0 20rpx;
}
.btns{
	color: #FFFFFF;
	background-color: #005EA1;
	font-size: 26rpx;
	height: 60rpx;
	width: 194rpx;
	border-radius: 10rpx;
}
.videoBox{
	position: absolute;
	top: 0;
	width: 750rpx;
	height: 100%;
}
.videoBoxs{
	position: absolute;
	top: 0;
	width: 750rpx;
	height: 100%;
}
.chooseContent{
	position: absolute;
	top: 50%;
	
	left: 50%;
	-webkit-transform: translate(-50%);
	        transform: translate(-50%);
	background-color: #FFFFFF;
	border-radius: 10rpx;
	width:calc( 100% - 140rpx);
	padding-bottom: 40rpx;
}
video{
	position: absolute;
	top: 50%;
	left: 0;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	width: 100%;
	z-index: 10000;
}
.closeBtn{
	width: 50rpx;
	height: 50rpx;
}
.close{
	font-size: 28rpx;
	line-height: 80rpx;
	margin-top: 20px;
	background-color: #FFFFFF;
	width: 120rpx;
	text-align: center;
	border-radius: 10rpx;
	height: 80rpx;
	text-align: center;
}
.closeImg{
	width: 50rpx;
	height: 50rpx;
}
.title{
	height: 100rpx;
	line-height: 100rpx;
}
.box{
	height: 80rpx;
	width: 462rpx;
	line-height: 80rpx;
	text-align: center;
	margin-left: 90rpx;
	border: 2rpx solid#000000;
	border-radius: 5rpx;
	margin-bottom: 10rpx;
}

