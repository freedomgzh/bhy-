/* 订单 */
.order {
	margin: 20rpx 20rpx 0 20rpx;
	background-color: #FFFFFF;
	border-radius: 8rpx;
}
.label {
	text-align: center;
}
.tips {
	height: 88rpx;
	line-height: 88rpx;
	border-bottom: 2rpx solid#EEEEEE;
	padding: 0 20rpx;
}
.ons {
	font-size: 30rpx;
}
.tns {
	font-size: 26rpx;
	color: #666666;
}
.orderList {
	font-size: 28rpx;
	height: 141rpx;
	padding: 0 20rpx;
}
.orderList text {
	text-align: center;
}
.num {
	font-size: 38rpx;
}
.set{
	color: #FFFFFF;
	height: 50rpx;
	width: 190rpx;
	text-align: center;
	border: 2rpx solid#FFFFFF;
	border-radius: 10rpx;
	line-height: 50rpx;
	font-size: 26rpx;
	margin-right: 43rpx;
}

